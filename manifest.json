{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "First App",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*",
        "https://open.spotify.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://open.spotify.com/*"],
            "js": ["src/scripts/test.ts"]
        }
    ]
}